{
    "name": "devuniverse/filemanager",
    "description": "A laravel file manager",
    "homepage": "https://github.com/devuniverse/filemanager",
    "keywords": [
        "file",
        "file amanger",
        "laravel files",
        "laravel file manager"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Devuniverse",
            "email": "ishimwe@gmail.com"
        },
        {
            "name": "Laravrix",
            "email": "laravrix@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "Devuniverse\\Filemanager\\": "/src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
      },
      "extra": {
          "laravel": {
              "providers": [
                  "Devuniverse\\Filemanager\\FilemanagerServiceProvider"
              ]
          }
      },
      "minimum-stability": "stable",
      "require": {
        "laravel/framework": "^6.0",
        "intervention/image": "^2.5"
      },
      "require-dev": {
          "laravel/laravel": "^6.0"
    }
}
